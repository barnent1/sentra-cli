{
  "name": "@sentra/cli",
  "version": "0.1.0",
  "description": "The intelligent Claude Code command center for autonomous project execution",
  "main": "dist/index.js",
  "bin": {
    "sentra": "dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "ai",
    "claude-code",
    "autonomous-development",
    "project-orchestration",
    "mcp",
    "cli",
    "personas",
    "automation"
  ],
  "author": "Sentra Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barnent1/sentra-cli.git"
  },
  "bugs": {
    "url": "https://github.com/barnent1/sentra-cli/issues"
  },
  "homepage": "https://github.com/barnent1/sentra-cli#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.15",
    "axios": "^1.6.8",
    "ws": "^8.16.0",
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "dotenv": "^16.4.5",
    "yaml": "^2.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "minimatch": "^9.0.4",
    "semver": "^7.6.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "winston": "^3.13.0",
    "joi": "^17.12.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/pg": "^8.11.5",
    "@types/fs-extra": "^11.0.4",
    "@types/uuid": "^9.0.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.4.5",
    "rollup": "^4.17.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-banner": "^0.2.1",
    "vitest": "^1.5.0",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "claude-code": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}